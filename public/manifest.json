{
  "manifest_version": 2,
  "name": "Reminder",
  "author": "alexlabrioche",
  "version": "0.1",
  "description": "Replace new tab screen with minimalist scheduled reminder to browse your favorite websites",
  "icons": {
    "128": "square.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk'; object-src 'self'",
  "permissions": ["storage", "tabs", "topSites"]
}
